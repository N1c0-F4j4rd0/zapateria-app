{
  "name": "zapateria-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' src/server.ts",
    "dev:alt": "ts-node-esm src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test:e2e": "jest --runInBand --detectOpenHandles"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cookie-session": "^2.0.45",
    "@types/cors": "^2.8.19",
    "@types/csurf": "^1.11.5",
    "@types/express": "^4.17.25",
    "@types/express-serve-static-core": "^4.19.7",
    "@types/serve-static": "^1.15.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
